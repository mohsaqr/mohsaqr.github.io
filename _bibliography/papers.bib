  year = {2022},
  title = {Instant or Distant: A Temporal Network Tale of Two Interaction Platforms and Their Influence on Collaboration},
  journal = {Educating for a New Future: Making Sense of Technology-Enhanced Learning Adoption. EC-TEL 2022. Lecture Notes in Computer Science},
  booktitle = {Educating for a New Future: Making Sense of Technology-Enhanced Learning Adoption. EC-TEL 2022. Lecture Notes in Computer Science},
