@BOOK{López-Pernas2024355,
	author = {López-Pernas, Sonsoles and Saqr, Mohammed},
	title = {Modeling the Dynamics of Longitudinal Processes in Education. A Tutorial with R for the VaSSTra Method},
	year = {2024},
