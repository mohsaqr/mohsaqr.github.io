  journal = {Proceedings - IEEE 24st International Conference on Advanced Learning Technologies, ICALT 2024},
  booktitle = {Proceedings - IEEE 24st International Conference on Advanced Learning Technologies, ICALT 2024},
  volume = {NULL},
  pages = {86--90},
