@ARTICLE{Bermo2021,
	author = {Bermo, Mohammed and Saqr, Mohammed and Hoffman, Hunter and Patterson, David and Sharar, Sam and Minoshima, Satoshi and Lewis, David H.},
	title = {Utility of SPECT Functional Neuroimaging of Pain},
	year = {2021},
	journal = {Frontiers in Psychiatry},
	volume = {12},
	doi = {10.3389/fpsyt.2021.705242},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85112432631&doi=10.3389%2ffpsyt.2021.705242&partnerID=40&md5=1758ca6cf3a2dbf41186e298bb8e4041},
	abstract = {Functional neuroimaging modalities vary in spatial and temporal resolution. One major limitation of most functional neuroimaging modalities is that only neural activation taking place inside the scanner can be imaged. This limitation makes functional neuroimaging in many clinical scenarios extremely difficult or impossible. The most commonly used radiopharmaceutical in Single Photon Emission Tomography (SPECT) functional brain imaging is Technetium 99 m-labeled Ethyl Cysteinate Dimer (ECD). ECD is a lipophilic compound with unique pharmacodynamics. It crosses the blood brain barrier and has high first pass extraction by the neurons proportional to regional brain perfusion at the time of injection. It reaches peak activity in the brain 1 min after injection and is then slowly cleared from the brain following a biexponential mode. This allows for a practical imaging window of 1 or 2 h after injection. In other words, it freezes a snapshot of brain perfusion at the time of injection that is kept and can be imaged later. This unique feature allows for designing functional brain imaging studies that do not require the patient to be inside the scanner at the time of brain activation. Functional brain imaging during severe burn wound care is an example that has been extensively studied using this technique. Not only does SPECT allow for imaging of brain activity under extreme pain conditions in clinical settings, but it also allows for imaging of brain activity modulation in response to analgesic maneuvers whether pharmacologic or non-traditional such as using virtual reality analgesia. Together with its utility in extreme situations, SPECTS is also helpful in investigating brain activation under typical pain conditions such as experimental controlled pain and chronic pain syndromes. © Copyright © 2021 Bermo, Saqr, Hoffman, Patterson, Sharar, Minoshima and Lewis.},
	author_keywords = {brain; ECD; functional imaging; pain; SPECT},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {All Open Access, Gold Open Access, Green Open Access}
}

