@ARTICLE{Saqr2022,
	author = {Saqr, Mohammed and Tuominen, Ville and Valtonen, Teemu and Sointu, Erkko and Väisänen, Sanna and Hirsto, Laura},
	title = {Teachers’ Learning Profiles in Learning Programming: The Big Picture!},
	year = {2022},
