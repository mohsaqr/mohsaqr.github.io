  year = {2023},
  title = {Mapping students’ temporal pathways in a computational thinking escape room},
  journal = {Proceedings of the 1st Finnish Learning Analytics and Artificial Intelligence in Education Conference (FLAIEC 2022) (CEUR Workshop Proceedings)},
